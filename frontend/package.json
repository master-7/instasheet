{
  "name": "instasheet",
  "version": "0.0.1",
  "description": "The Instagram photo search system",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --content-base public/ --env=development --hot --colors",
    "start-prod": "webpack-dev-server --progress --inline --content-base public/ --env=production --env=production --hot --colors",
    "build-dev": "webpack --config ./webpack.config.development.js --progress --env=development -p",
    "build": "webpack --config ./webpack.config.production.js --progress --env=production -p",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "contributors": [
    "NickMasters",
    "MaxSharov",
    "DmitriyNikolaev"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/master-7/instasheet"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babel-polyfill": "^6.23.0",
    "enzyme": "^2.9.1",
    "eslint-loader": "^1.8.0",
    "history": "^4.6.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-if": "^2.1.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.10",
    "redux": "^3.7.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "resolve-url-loader": "^2.1.0",
    "styled-components": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.1.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.48.0",
    "html-webpack-plugin": "^2.29.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "redux-logger": "^3.0.6",
    "url-loader": "^0.5.9",
    "webpack": "^3.0.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.png$": "<rootDir>/mocks/image.js"
    }
  }
}
